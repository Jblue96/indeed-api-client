{
  "name": "indeed-api-client",
  "version": "1.0.0",
  "description": "A client library for connecting to the Indeed API",
  "main": "./dist/bundle.js",
  "babel": {
    "presets": ["es2015"]
  },
  "scripts": {
    "build": "babel-node tools/build.js",
    "start": "webpack-dev-server --progress --profile --colors --hot",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "prebuild": "npm-run-all clean-dist",
    "build": "babel-node tools/build.js",
    "prepublish": "npm run build"
  },
  "author": "thedrew12",
  "license": "MIT",
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "query-string": "^4.2.2",
    "webpack-dev-server": "^1.14.1"
  },
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.0.3",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "6.8.0",
    "colors": "^1.1.2",
    "copy-webpack-plugin": "^3.0.1",
    "eslint": "^2.8.0",
    "eslint-plugin-react": "^5.0.1",
    "npm-run-all": "2.3.0",
    "rimraf": "2.5.2",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thedrew12/indeed-api-client"
  }
}
